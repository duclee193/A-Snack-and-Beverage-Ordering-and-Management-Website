{
    "name": "duclee193/snack-and-beverage-website",
    "description": "A snack and beverage ordering and management website.",
    "type": "project",
    "license": "MIT",
    "require": {
      "php": ">=8.0",
      "phpmailer/phpmailer": "^6.8",
      "vlucas/phpdotenv": "^5.5",
      "monolog/monolog": "^3.0"
    },
    "autoload": {
      "psr-4": {
        "App\\": "admin/",
        "Api\\": "api/",
        "Cart\\": "cart/",
        "Database\\": "database/",
        "Layout\\": "layout/",
        "Login\\": "login/",
        "Menu\\": "menu-con/",
        "Utils\\": "utils/"
      }
    },
    "scripts": {
      "test": "vendor/bin/phpunit"
    },
    "config": {
      "optimize-autoloader": true
    }
  }
  